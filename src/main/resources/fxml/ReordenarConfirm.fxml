<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="150.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sistema.ui.ReordenarController">
   <children>
      <Pane prefHeight="200.0" prefWidth="200.0">
         <children>
            <Label fx:id="proveedorNameLabel" layoutX="103.0" layoutY="75.0" prefHeight="17.0" prefWidth="175.0" text="ProveedorName" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="Corbel Bold" size="14.0" />
               </font>
            </Label>
            <ImageView fitHeight="76.0" fitWidth="76.0" layoutX="14.0" layoutY="37.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/Check.png" />
               </image>
            </ImageView>
            <Label layoutX="128.0" layoutY="29.0" prefHeight="17.0" prefWidth="125.0" text="Producto reordenado">
               <font>
                  <Font name="Corbel" size="14.0" />
               </font>
            </Label>
            <Button fx:id="btnAceptar" layoutX="158.0" layoutY="109.0" mnemonicParsing="false" onAction="#btnAceptarAction" style="-fx-background-color: #5882E8; -fx-background-radius: 15;" text="Aceptar" textFill="WHITE">
               <font>
                  <Font name="Corbel Bold" size="14.0" />
               </font>
            </Button>
            <Label layoutX="111.0" layoutY="46.0" prefHeight="17.0" prefWidth="175.0" text="Correo enviado a Proveedor: ">
               <font>
                  <Font name="Corbel" size="14.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</VBox>
